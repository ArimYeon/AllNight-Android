<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".LoginActivity" android:background="@color/backgound">

    <Button
            android:text="@string/start"
            android:layout_width="328dp"
            android:layout_height="52dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/login_button" app:layout_constraintHorizontal_bias="0.506"
            android:layout_marginBottom="40dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="16dp" android:layout_marginEnd="16dp"
            android:backgroundTint="@color/button_backg"
            android:textSize="19sp" android:textColor="#FFFFFF" android:background="@drawable/ic_9_patch"
            android:fontFamily="@font/nanumbarungothicbold"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_logo_allnight"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/imageView" android:layout_marginStart="67dp"
            android:layout_marginEnd="67dp" android:layout_marginTop="229dp" app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>